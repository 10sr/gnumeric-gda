#: src/functions/fn-string.c:483
msgid ""
"@FUNCTION=FIXED\n"
"@SYNTAX=FIXED(num,[decimals, no_commas])\n"
"@DESCRIPTION=FIXED returns @num as a formatted string with @decimals numbers "
"after the decimal point, omitting commas if requested by @no_commas.\n"
"@EXAMPLES=\n"
"FIXED(1234.567,2) equals \"1,234.57\".\n"
"\n"
"@SEEALSO="
msgstr ""
"@FUNCTION=FIXED\n"
"@SYNTAX=FIXED(число, [десятков╕, без_ком])\n"
"@DESCRIPTION=Функц╕я FIXED поверта╓ @число, як форматований рядок з "
"@десятков╕ цифр п╕сля десятково╖ точки, якщо вказано @без_ком - розд╕лювач╕ "
"тисяч опускаються.\n"
"@EXAMPLES=\n"
"FIXED(1234.567,2) дор╕вню╓ \"1,234.57\".\n"
"\n"
"@SEEALSO="

#: src/functions/fn-string.c:587
msgid ""
"@FUNCTION=PROPER\n"
"@SYNTAX=PROPER(string)\n"
"@DESCRIPTION=PROPER returns @string with initial of each word capitalised.\n"
"@EXAMPLES=\n"
"PROPER(\"j. f. kennedy\") equals \"J. F. Kennedy\".\n"
"\n"
"@SEEALSO=LOWER, UPPER"
msgstr ""
"@FUNCTION=PROPER\n"
"@SYNTAX=PROPER(рядок)\n"
"@DESCRIPTION=Поверта╓ рядок @рядок з перетвориними у велик╕ першими л╕терами "
"кожного слова.\n"
"@EXAMPLES=\n"
"PROPER(\"т.г.шевченко\") дор╕вню╓ Т.Г.Шевченко.\n"
"\n"
"@SEEALSO=LOWER, UPPER"

#: src/functions/fn-string.c:632
msgid ""
"@FUNCTION=REPLACE\n"
"@SYNTAX=REPLACE(old,start,num,new)\n"
"@DESCRIPTION=REPLACE returns @old with @new replacing @num characters from "
"@start.\n"
"@EXAMPLES=\n"
"REPLACE(\"testing\",2,3,\"*****\") equals \"t*****ing\".\n"
"\n"
"@SEEALSO=MID, SEARCH, SUBSTITUTE, TRIM"
msgstr ""

#: src/functions/fn-string.c:685
msgid ""
"@FUNCTION=T\n"
"@SYNTAX=T(value)\n"
"@DESCRIPTION=T returns @value if and only if it is text, otherwise a blank "
"string.\n"
"@EXAMPLES=\n"
"T(\"text\") equals \"text\".\n"
"T(64) returns an empty cell.\n"
"\n"
"@SEEALSO=CELL, N, VALUE"
msgstr ""

#: src/functions/fn-string.c:709
msgid ""
"@FUNCTION=TEXT\n"
"@SYNTAX=TEXT(value,format_text)\n"
"@DESCRIPTION=TEXT returns @value as a string with the specified format.\n"
"@EXAMPLES=\n"
"TEXT(3.223,\"$0.00\") equals \"$3.22\".\n"
"TEXT(date(1999,4,15),\"mmmm, dd, yy\") equals \"April, 15, 99\".\n"
"\n"
"@SEEALSO=DOLLAR"
msgstr ""
"@FUNCTION=TEXT\n"
"@SYNTAX=TEXT(value,format_text)\n"
"@DESCRIPTION=TEXT повета╓ @value як рядок у вказаному формат╕.\n"
"@EXAMPLES=\n"
"TEXT(3.223,\"$0.00\") р╕вне \"$3.22\".\n"
"TEXT(date(1999,4,15),\"mmmm, dd, yy\") р╕вне \"April, 15, 99\".\n"
"\n"
"@SEEALSO=DOLLAR"

#: src/functions/fn-string.c:765
msgid ""
"@FUNCTION=EXPRESSION\n"
"@SYNTAX=EXPRESSION(cell)\n"
"@DESCRIPTION=EXPRESSION returns expression in @cell as a string, orempty if "
"the cell is not an expression.\n"
"@EXAMPLES=\n"
"in A1 EXPRESSION(A2) equals 'EXPRESSION(A3)'.\n"
"in A2 EXPRESSION(A3) equals empty.\n"
"\n"
"@SEEALSO=TEXT"
msgstr ""

#: src/functions/fn-string.c:810
msgid ""
"@FUNCTION=TRIM\n"
"@SYNTAX=TRIM(text)\n"
"@DESCRIPTION=TRIM returns @text with only single spaces between words.\n"
"@EXAMPLES=\n"
"TRIM(\"  a bbb  cc\") equals \"a bbb cc\".\n"
"\n"
"@SEEALSO=CLEAN, MID, REPLACE, SUBSTITUTE"
msgstr ""
"@FUNCTION=TRIM\n"
"@SYNTAX=TRIM(text)\n"
"@DESCRIPTION=TRIM поверта╓ @text з одинарними пропусками м╕ж словами.\n"
"@EXAMPLES=\n"
"TRIM(\"  a bbb  cc\") р╕вне \"a bbb cc\".\n"
"\n"
"@SEEALSO=CLEAN, MID, REPLACE, SUBSTITUTE"

#: src/functions/fn-string.c:855
msgid ""
"@FUNCTION=VALUE\n"
"@SYNTAX=VALUE(text)\n"
"@DESCRIPTION=VALUE returns numeric value of @text.\n"
"@EXAMPLES=\n"
