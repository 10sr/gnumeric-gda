#: src/functions/fn-string.c:422 src/functions/fn-string.c:462
msgid "Invalid argument"
msgstr "Ugyldig argument"

#: src/functions/fn-string.c:358
msgid ""
"@FUNCTION=CLEAN\n"
"@SYNTAX=CLEAN(string)\n"
"@DESCRIPTION=Cleans the string from any non-printable characters.\n"
"@SEEALSO="
msgstr ""
"@FUNCTION=CLEAN\n"
"@SYNTAX=CLEAN(string)\n"
"@DESCRIPTION=Fjerner ethvert ikke-skrivbart tegn fra strengen.\n"
"@SEEALSO="

#: src/functions/fn-string.c:394
msgid ""
"@FUNCTION=FIND\n"
"@SYNTAX=FIND(string1,string2[,start])\n"
"@DESCRIPTION=Returns position of @string1 in @string2 (case-sesitive), "
"searching only from character @start onwards (assumed 1 if omitted).\n"
"@SEEALSO=EXACT, LEN, MID, SEARCH"
msgstr ""
"@FUNCTION=FIND\n"
"@SYNTAX=FIND(streng1,streng2[,start])\n"
"@DESCRIPTION=Returnerer posisjonen til @string1 i @string2 (skiller mellom "
"små og store bokstaver), søker bare fra tegnet @start og utover (bruker 1 "
"hvis utelatt).\n"
"@SEEALSO=EXACT, LEN, MID, SEARCH"

#: src/functions/fn-string.c:428
msgid ""
"@FUNCTION=FIXED\n"
"@SYNTAX=FIXED(num, [decimals, no_commas])\n"
"@DESCRIPTION=Returns @num as a formatted string with @decimals numbers after "
"the decimal point, omitting commas if requested by @no_commas.\n"
"@SEEALSO="
msgstr ""
"@FUNCTION=FIXED\n"
"@SYNTAX=FIXED(num, [desimaler, ikke_komma])\n"
"@DESCRIPTION=Returnerer @num som em formatert streng med @desimaler tall "
"etter komma, og utelater komma hvis det etterspørres med @ikke_komma.\n"
"@SEEALSO="

#: src/functions/fn-string.c:535
msgid ""
"@FUNCTION=PROPER\n"
"@SYNTAX=PROPER(string)\n"
"@DESCRIPTION=Returns @string with initial of each word capitalised.\n"
"@SEEALSO=LOWER, UPPER"
msgstr ""
"@FUNCTION=PROPER\n"
"@SYNTAX=PROPER(streng)\n"
"@DESCRIPTION=Returnerer @streng med stor bokstav i begynnelsen av hvert "
"ord.\n"
"@SEEALSO=LOWER, UPPER"

#: src/functions/fn-string.c:574
msgid ""
"@FUNCTION=REPLACE\n"
"@SYNTAX=REPLACE(old,start,num,new)\n"
"@DESCRIPTION=Returns @old with @new replacing @num characters from @start.\n"
"@SEEALSO=MID, SEARCH, SUBSTITUTE, TRIM"
msgstr ""
"@FUNCTION=REPLACE\n"
"@SYNTAX=REPLACE(gml,start,num,ny)\n"
"@DESCRIPTION=Returnerer @gml hvor @ny erstatter @num tegn fra @start.\n"
"@SEEALSO=MID, SEARCH, SUBSTITUTE, TRIM"

#: src/functions/fn-string.c:625
msgid ""
"@FUNCTION=T\n"
"@SYNTAX=T(value)\n"
"@DESCRIPTION=Returns @value if and only if it is text, otherwise a blank "
"string.\n"
"@SEEALSO=CELL, N, VALUE"
msgstr ""
"@FUNCTION=T\n"
"@SYNTAX=T(verdi)\n"
"@DESCRIPTION=Returns @verdi hvis og bare hvis den er av tekst, ellers "
"returnerer den en tom streng.\n"
"@SEEALSO=CELL, N, VALUE"

#: src/functions/fn-string.c:645
msgid ""
"@FUNCTION=TEXT\n"
"@SYNTAX=TEXT(value,format_text)\n"
"@DESCRIPTION=Returns @value as a string with the specified format.\n"
"@SEEALSO=DOLLAR"
msgstr ""
"@FUNCTION=TEXT\n"
"@SYNTAX=TEXT(verdi,format_tekst)\n"
"@DESCRIPTION=Returnerer @verdi som en streng med spesifisert format.\n"
"@SEEALSO=DOLLAR"

#: src/functions/fn-string.c:698
msgid ""
"@FUNCTION=TRIM\n"
"@SYNTAX=TRIM(text)\n"
"@DESCRIPTION=Returns @text with only single spaces between words.\n"
"@SEEALSO=CLEAN, MID, REPLACE, SUBSTITUTE"
msgstr ""
"@FUNCTION=TRIM\n"
"@SYNTAX=TRIM(tekst)\n"
"@DESCRIPTION=Returnerer @tekst med kun enkle mellomrom mellom ord.\n"
"@SEEALSO=CLEAN, MID, REPLACE, SUBSTITUTE"

#: src/functions/fn-string.c:743
msgid ""
"@FUNCTION=VALUE\n"
"@SYNTAX=VALUE(text)\n"
"@DESCRIPTION=Returns numeric value of @text.\n"
"@SEEALSO=DOLLAR, FIXED, TEXT"
msgstr ""
"@FUNCTION=VALUE\n"
"@SYNTAX=VALUE(tekst)\n"
"@DESCRIPTION=Returnerer numerisk verdi av @tekst.\n"
"@SEEALSO=DOLLAR, FIXED, TEXT"

#: src/functions/fn-string.c:824
msgid ""
"@FUNCTION=SUBSTITUTE\n"
"@SYNTAX=SUBSTITUTE(text, old, new [,num])\n"
"@DESCRIPTION=Replaces @old with @new in @text.  Substitutions are only "
"applied to instance @num of @old in @text, otherwise every one is changed.\n"
"@SEEALSO=REPLACE, TRIM"
msgstr ""
"@FUNCTION=SUBSTITUTE\n"
"@SYNTAX=SUBSTITUTE(tekst, gml, ny [,num])\n"
"@DESCRIPTION=Erstatter @gml med @ny i @tekst. Utskifting utføres bare på "
"forekomst @num av @gml i @tekst, ellers byttes alle ut.\n"
"@SEEALSO=REPLACE, TRIM"

#: src/functions/fn-string.c:895
msgid ""
"@FUNCTION=DOLLAR\n"
"@SYNTAX=DOLLAR(num,[decimals])\n"
"@DESCRIPTION=Returns @num formatted as currency.\n"
"@SEEALSO=FIXED, TEXT, VALUE"
msgstr ""
"@FUNCTION=DOLLAR\n"
"@SYNTAX=DOLLAR(ant,[desimaler])\n"
"@DESCRIPTION=Returnerer @ant formatert som valuta.\n"
"@SEEALSO=FIXED, TEXT, VALUE"

#: src/functions/fn-string.c:960
msgid ""
"@FUNCTION=SEARCH\n"
"@SYNTAX=SEARCH(text,within[,start_num])\n"
"@DESCRIPTION=Returns the location of a character or text string within "
"another string.  @text is the string or character to be searched. @within is "
"the string in which you want to search.  @start_num is the start position of "
"the search in @within.  If @start_num is omitted, it is assumed to be one.  "
"The search is not case sensitive. \n"
"@text can contain wildcard characters (*) and question marks (?) to control "
"the search.  A question mark matches with any character and wildcard matches "
"with any string including empty string.  If you want the actual wildcard or "
"question mark to be searched, use tilde (~) before the character. \n"
"If @text is not found, SEARCH returns #VALUE! error. If @start_num is less "
"than one or it is greater than the length of @within, SEARCH returns #VALUE! "
"error. \n"
"@SEEALSO=FIND"
msgstr ""
"@FUNCTION=SEARCH\n"
"@SYNTAX=SEARCH(tekst,innen[,start_num])\n"
"@DESCRIPTION=Returnerer plasseringen av et tegn eller tekst streng inni en "
"annen streng.  @tekst er strengen eller tegnet det skal søkes etter. @innen "
"er strengen du skal søke i. @start_num er startposisjonen for søket i "
"@innen.  Hvis @start_num er utelatt blir den regnet som one.  Søket er gjør "
"ikke forskjell på små og store bokstaver. \n"
"@tekst kan inneholde wildcard tegn (*) og spørsmålstegn (?) for å "
"kontrollerer søket. Et spørsmålstegn erstatter ethvert tegn og wildcard "
"erstatter enhver streng inkludert en tom streng.  Hvis du skal søke på selve "
"wildcard eller spørsmålstegn tegnene, må du bruke tilde (~) foran tegnet. \n"
"Hvis @tekst ikke blir funnet, returnerer SEARCH en #VALUE! feil. Hvis "
"@start_num er mindre enn en eller større enn lengden av @innen, returnerer "
"SEARCH en #VALUE! feil. \n"
"@SEEALSO=FIND"

