"2   COUNT\n"
"3   COUNTA\n"
"4   MAX\n"
"5   MIN\n"
"6   PRODUCT\n"
"7   STDEV\n"
"8   STDEVP\n"
"9   SUM\n"
"10   VAR\n"
"11   VARP\n"
"\n"
"@SEEALSO=COUNT,SUM"
msgstr ""

#: src/fn-math.c:2448
msgid ""
"@FUNCTION=SERIESSUM\n"
