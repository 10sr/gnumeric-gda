"@FUNCTION=NA\n"
"@SYNTAX=NA()\n"
"@DESCRIPTION=NA returns the error value #N/A. \n"
"@SEEALSO="
msgstr ""
"@FUNCTION=NA\n"
"@SYNTAKS=NA()\n"
"@DESCRIPTION=NA returnerer feilverdien #N/A. \n"
"@SEEALSO="

#: src/functions/fn-misc.c:185
msgid ""
"@FUNCTION=ERROR\n"
"@SYNTAX=ERROR(text)\n"
"@DESCRIPTION=Return the specified error\n"
"\n"
