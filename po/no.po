msgstr ""
"\n"
"@EXAMPLES=\n"
"DSTDEVP(A1:C7, \"Alder\", A9:B11) er lik 7.\n"
"DSTDEVP(A1:C7, \"Lønn\", A9:B11) er lik 6459,5.\n"
"\n"
"@SEEALSO=DSTDEV"

#: plugins/fn-database/functions.c:625
msgid ""
"@FUNCTION=DSUM\n"
"@SYNTAX=DSUM(database,field,criteria)\n"
"@DESCRIPTION=DSUM function returns the sum of numbers in a column that match "
"conditions specified.\n"
"\n"
msgstr ""
"@FUNCTION=DSUM\n"
"@SYNTAX=DSUM(database,felt,kriterie)\n"
"@DESCRIPTION=DSUM-funksjonen returnerer summen av tallene i en kolonne som "
"møter de spesifiserte betingelsene.\n"
"\n"

#: plugins/fn-database/functions.c:633
msgid ""
"\n"
"@EXAMPLES=\n"
"DSUM(A1:C7, \"Age\", A9:B11) equals 72.\n"
"DSUM(A1:C7, \"Salary\", A9:B11) equals 81565.\n"
