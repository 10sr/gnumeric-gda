msgstr ""
"@FUNCTION=ISERROR\n"
"@SYNTAX=ISERROR(exp)\n"
"@DESCRIPTION=Returnerer en SANN verdi hvis uttrykket har en feil\n"
"\n"
"@SEEALSO=ERROR"

#: src/functions/fn-misc.c:65
msgid ""
"@FUNCTION=ISNA\n"
"@SYNTAX=ISNA()\n"
"@DESCRIPTION=ISNA returns TRUE if the value is the #N/A error value. \n"
"@SEEALSO="
msgstr ""
"@FUNCTION=ISNA\n"
"@SYNTAX=ISNA()\n"
"@DESCRIPTION=Returnerer SANN hvis verdien er #N/A feilverdien. \n"
"@SEEALSO="

#: src/functions/fn-misc.c:95
msgid ""
"@FUNCTION=ISERR\n"
"@SYNTAX=ISERR()\n"
"@DESCRIPTION=ISERR returns TRUE if the value is any error value except #N/A. "
