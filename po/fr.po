#: src/functions/fn-database.c:492
msgid ""
"@FUNCTION=DAVERAGE\n"
"@SYNTAX=DAVERAGE(database,field,criteria)\n"
"@DESCRIPTION=DAVERAGE function returns the average of the values in a list "
"or database that match conditions specified. \n"
msgstr ""
"@FUNCTION=DAVERAGE\n"
"@SYNTAX=DAVERAGE(base_de_données,champ,critère)\n"
"@DESCRIPTION=La fonction DAVERAGE retourne la moyenne des valeurs d'une "
"liste ou d'une base de données qui remplissent les conditions spécifiées.\n"

#: src/functions/fn-database.c:500
msgid ""
"\n"
"DAVERAGE(A1:C7, \"Salary\", A9:A11) equals 42296.3333.\n"
"DAVERAGE(A1:C7, \"Age\", A9:A11) equals 39.\n"
"DAVERAGE(A1:C7, \"Salary\", A9:B11) equals 40782.5.\n"
"DAVERAGE(A1:C7, \"Age\", A9:B11) equals 36.\n"
"\n"
"@SEEALSO=DCOUNT"
msgstr ""

#: src/functions/fn-database.c:561
msgid ""
"@FUNCTION=DCOUNT\n"
"@SYNTAX=DCOUNT(database,field,criteria)\n"
"@DESCRIPTION=DCOUNT function counts the cells that contain numbers in a "
"database that match conditions specified. \n"
msgstr ""
"@FUNCTION=DCOUNT\n"
"@SYNTAX=DCOUNT(base_de_données,champ,critère)\n"
"@DESCRIPTION=La fonction DCOUNT compte les cellules qui contiennent des "
"nombres dans une base de données qui remplit les conditions spécifiées.\n"

#: src/functions/fn-database.c:569
msgid ""
"\n"
"DCOUNT(A1:C7, \"Salary\", A9:A11) equals 3.\n"
"DCOUNT(A1:C7, \"Salary\", A9:B11) equals 2.\n"
"DCOUNT(A1:C7, \"Name\", A9:B11) equals 0.\n"
"\n"
"@SEEALSO=DAVERAGE"
msgstr ""

#: src/functions/fn-database.c:622
msgid ""
"@FUNCTION=DCOUNTA\n"
"@SYNTAX=DCOUNTA(database,field,criteria)\n"
"@DESCRIPTION=DCOUNTA function counts the cells that contain data in a "
"database that match conditions specified. \n"
msgstr ""
"@FUNCTION=DCOUNTA\n"
"@SYNTAX=DCOUNTA(base_de_données,champ,critère)\n"
"@DESCRIPTION=La fonction DCOUNTA compte les cellules qui contiennent des "
"données dans une base de données qui remplit les conditions spécifiées.\n"

#: src/functions/fn-database.c:630
msgid ""
"\n"
"DCOUNTA(A1:C7, \"Salary\", A9:A11) equals 3.\n"
"DCOUNTA(A1:C7, \"Salary\", A9:B11) equals 2.\n"
"DCOUNTA(A1:C7, \"Name\", A9:B11) equals 2.\n"
"\n"
"@SEEALSO=DCOUNT"
msgstr ""

#: src/functions/fn-database.c:680
msgid ""
"@FUNCTION=DGET\n"
"@SYNTAX=DGET(database,field,criteria)\n"
"@DESCRIPTION=DGET function returns a single value from a column that match "
"conditions specified. \n"
msgstr ""
"@FUNCTION=DGET\n"
"@SYNTAX=DGET(base_de_données,champ,critère)\n"
"@DESCRIPTION=La fonction DGET retourne une seule valeur d'une colonne qui "
"remplit les conditions spécifiées.\n"

#: src/functions/fn-database.c:688
msgid ""
"\n"
"DGET(A1:C7, \"Salary\", A9:A10) equals 34323.\n"
"DGET(A1:C7, \"Name\", A9:A10) equals \"Clark\".\n"
"\n"
"If none of the items match the conditions, DGET returns #VALUE! error. If "
"more than one items match the conditions, DGET returns #NUM! error. \n"
"@SEEALSO=DCOUNT"
msgstr ""

