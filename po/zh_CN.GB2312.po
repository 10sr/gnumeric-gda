#: src/fn-string.c:479
msgid "Invalid argument"
msgstr "参数无效"

#: src/fn-string.c:373
msgid ""
"@FUNCTION=CLEAN\n"
"@SYNTAX=CLEAN(string)\n"
"@DESCRIPTION=Cleans the string from any non-printable characters.\n"
"@SEEALSO="
msgstr ""
"@FUNCTION=CLEAN\n"
"@SYNTAX=CLEAN(string)\n"
"@DESCRIPTION=CLEAN清除字符串中任何非打印字符\n"
"@SEEALSO="

#: src/fn-string.c:412
msgid ""
"@FUNCTION=FIND\n"
"@SYNTAX=FIND(string1,string2[,start])\n"
"@DESCRIPTION=Returns position of @string1 in @string2 "
"(case-sesitive), searching only from character @start onwards (assumed 1 "
"if omitted)\n"
"@SEEALSO=EXACT, LEN, MID, SEARCH"
msgstr ""
"@FUNCTION=FIND\n"
"@SYNTAX=FIND(string1,string2[,start])\n"
"@DESCRIPTION=FIND函数在@string2中查找@string1(大小写敏感),"
"查找仅从@start开始,如果该参数被忽略,则从1开始查找"
"@SEEALSO=EXACT, LEN, MID, SEARCH"

#: src/fn-string.c:451
msgid ""
"@FUNCTION=FIXED\n"
"@SYNTAX=FIXED(num, [decimals, no_commas])\n"
"@DESCRIPTION=Returns @num as a formatted string with @decimals numbers "
"after the decimal point, omitting commas if requested by @no_commas\n"
"@SEEALSO="
msgstr ""
"@FUNCTION=FIXED\n"
"@SYNTAX=FIXED(num, [decimals, no_commas])\n"
"@DESCRIPTION=\"返回用@decimals格式化的@no_commas串\n"
"在小数点后,如果@no_commas需要忽略逗号"
"@SEEALSO="

#: src/fn-string.c:556
msgid ""
"@FUNCTION=PROPER\n"
"@SYNTAX=PROPER(string)\n"
"@DESCRIPTION=Returns @string with initial of each word capitalised\n"
"@SEEALSO=LOWER, UPPER"
msgstr ""
"@FUNCTION=PROPER\n"
"@SYNTAX=PROPER(string)\n"
"@DESCRIPTION=返回每个单词第一个字母大写的@string\n"
"@SEEALSO=LOWER, UPPER"

#: src/fn-string.c:595
msgid ""
"@FUNCTION=REPLACE\n"
"@SYNTAX=REPLACE(old,start,num,new)\n"
"@DESCRIPTION=Returns @old with @new replacing @num characters from @start\n"
"@SEEALSO=MID, SEARCH, SUBSTITUTE, TRIM"
msgstr ""
"@FUNCTION=REPLACE\n"
"@SYNTAX=REPLACE(old,start,num,new)\n"
"@DESCRIPTION=返回用@new替换@old从@start起的@num个字符\n"
"@SEEALSO=MID, SEARCH, SUBSTITUTE, TRIM"

#: src/fn-string.c:644
msgid ""
"@FUNCTION=T\n"
"@SYNTAX=T(value)\n"
"@DESCRIPTION=Returns @value if and only if it is text, otherwise a "
"blank string\n"
"@SEEALSO=CELL, N, VALUE"
msgstr ""
"@FUNCTION=T\n"
"@SYNTAX=T(value)\n"
"@DESCRIPTION=如果是文本返回@value,否则返回空串\n"
"@SEEALSO=CELL, N, VALUE"

#: src/fn-string.c:667
msgid ""
"@FUNCTION=TRIM\n"
"@SYNTAX=TRIM(text)\n"
"@DESCRIPTION=Returns @text with only single spaces between words\n"
"@SEEALSO=CLEAN, MID, REPLACE, SUBSTITUTE"
msgstr ""
"@FUNCTION=TRIM\n"
"@SYNTAX=TRIM(text)\n"
"@DESCRIPTION=返回的@text单词间仅有一个空格\n"
"@SEEALSO=CLEAN, MID, REPLACE, SUBSTITUTE"

#: src/fn-string.c:714
msgid ""
"@FUNCTION=VALUE\n"
"@SYNTAX=VALUE(text)\n"
"@DESCRIPTION=Returns numeric value of @text\n"
"@SEEALSO=DOLLAR, FIXED, TEXT"
msgstr ""
"@FUNCTION=VALUE\n"
"@SYNTAX=VALUE(text)\n"
"@DESCRIPTION=返回文本的数字值\n"
"@SEEALSO=DOLLAR, FIXED, TEXT"

#: src/fn-string.c:775
msgid ""
"@FUNCTION=SUBSTITUTE\n"
"@SYNTAX=SUBSTITUTE(text, old, new [,num]\n"
"@DESCRIPTION=Replaces @old with @new in @text.  Substitutions are "
"only applied to instance @num of @old in @text, otherwise every one "
"is changed.\n"
"@SEEALSO=REPLACE, TRIM"
msgstr ""
"@FUNCTION=SUBSTITUTE\n"
"@SYNTAX=SUBSTITUTE(text, old, new [,num]\n"
"@DESCRIPTION=把@text中的@old串用@new替换"
"如果有num参数,替换只能最多num次,否则替换每一个"
"@SEEALSO=REPLACE, TRIM"

#: src/fn-string.c:845
msgid ""
"@FUNCTION=DOLLAR\n"
"@SYNTAX=DOLLAR(num,[decimals])\n"
"@DESCRIPTION=Returns @num formatted as currency \n"
"@SEEALSO=FIXED, TEXT, VALUE"
msgstr ""
"@FUNCTION=DOLLAR\n"
"@SYNTAX=DOLLAR(num,[decimals])\n"
"@DESCRIPTION=以货币格式返回 \n"
"@SEEALSO=FIXED, TEXT, VALUE"

