msgstr "Estadística"

#: src/functions/fn-string.c:23
msgid ""
"@FUNCTION=CHAR\n"
"@SYNTAX=CHAR(x)\n"
"@DESCRIPTION=CHAR returns the ASCII character represented by the number @x.\n"
"@EXAMPLES=\n"
"CHAR(65) equals A.\n"
"\n"
"@SEEALSO=CODE"
msgstr ""
"@FUNCIÓ=CHAR\n"
"@SINTAXI=CHAR(x)\n"
"@DESCRIPCIÓ=CHAR retorna el caràcter ASCII representat pel número @x.\n"
"@EXEMPLES=\n"
"CHAR(65) equival a A.\n"
"\n"
"@VEURE TAMBÉ=CODE"

#: src/functions/fn-string.c:49
msgid ""
"@FUNCTION=CODE\n"
"@SYNTAX=CODE(char)\n"
"@DESCRIPTION=CODE returns the ASCII number for the character @char.\n"
"@EXAMPLES=\n"
"CODE(\"A\") equals 65.\n"
"\n"
"@SEEALSO=CHAR"
msgstr ""
"@FUNCIÓ=CODE\n"
"@SINTAXI=CODE(char)\n"
"@DESCRIPCIÓ=CODE retorna el número ASCII pel caràcter @char.\n"
"@EXEMPLES=\n"
"CODE(\"A\") equival a 65.\n"
"\n"
"@VEURE TAMBÉ=CHAR"

#: src/functions/fn-string.c:76
msgid ""
"@FUNCTION=EXACT\n"
"@SYNTAX=EXACT(string1, string2)\n"
"@DESCRIPTION=EXACT returns true if @string1 is exactly equal to @string2 "
"(this routine is case sensitive).\n"
"@EXAMPLES=\n"
"EXACT(\"key\",\"key\") equals TRUE.\n"
"EXACT(\"key\",\"Key\") equals FALSE.\n"
"\n"
"@SEEALSO=LEN"
msgstr ""
"@FUNCIÓ=EXACT\n"
"@SINTAXI=EXACT(string1, string2)\n"
"@DESCRIPCIÓ=EXACT retorna TRUE si @string1 és exactament igual a @string2 "
"(aquesta rutina és sensible a CASE).\n"
"@EXEMPLES=\n"
"EXACT(\"key\",\"key\") equival a TRUE.\n"
"EXACT(\"key\",\"Key\") equival a FALSE.\n"
"\n"
"@VEURE TAMBÉ=LEN"

#: src/functions/fn-string.c:103
msgid ""
"@FUNCTION=LEN\n"
"@SYNTAX=LEN(string)\n"
"@DESCRIPTION=LEN returns the length in characters of the string @string.\n"
"@EXAMPLES=\n"
"len(\"Helsinki\") equals 8.\n"
"\n"
"@SEEALSO=CHAR, CODE"
msgstr ""
"@FUNCIÓ=LEN\n"
"@SINTAXI=LEN(string)\n"
"@DESCRIPCIÓ=LEN retorna la longitud en caràcters de la cadena @string.\n"
"@EXEMPLES=\n"
"len(\"Helsinki\") equival a 8.\n"
"\n"
"@VEURE TAMBÉ=CHAR, CODE"

#: src/functions/fn-string.c:127
msgid ""
"@FUNCTION=LEFT\n"
