"@SYNTAX=PRICE(anbringelse,forfald,rente,ydelse,indfrielse,frekvens,basis)\n"
"@DESCRIPTION=\n"
"@EXAMPLES=\n"
"\n"
"@SEEALSO="

#: src/functions/fn-financial.c:2436
msgid ""
"@FUNCTION=YIELD\n"
"@SYNTAX=YIELD(settle,mat,rate,price,redemption_price,frequency,basis)\n"
"@DESCRIPTION=\n"
"@EXAMPLES=\n"
"\n"
"@SEEALSO="
msgstr ""
"@FUNCTION=YIELD\n"
"@SYNTAX=YIELD(anbringelse,forfald,rente,pris,indfrielse,frekvens,basis)\n"
"@DESCRIPTION=\n"
"@EXAMPLES=\n"
"\n"
"@SEEALSO="

#: src/functions/fn-financial.c:2454
msgid ""
