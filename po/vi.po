msgid ""
"\n"
"@EXAMPLES=\n"
"DAVERAGE(A1:C7, \"Salary\", A9:A11) equals 42296.3333.\n"
"DAVERAGE(A1:C7, \"Age\", A9:A11) equals 39.\n"
"DAVERAGE(A1:C7, \"Salary\", A9:B11) equals 40782.5.\n"
"DAVERAGE(A1:C7, \"Age\", A9:B11) equals 36.\n"
"\n"
"@SEEALSO=DCOUNT"
msgstr ""
"\n"
"@EXAMPLES=\n"
"DAVERAGE(A1:C7, \"Salary\", A9:A11) bằng 42296.3333.\n"
"DAVERAGE(A1:C7, \"Age\", A9:A11) bằng 39.\n"
"DAVERAGE(A1:C7, \"Salary\", A9:B11) bằng 40782.5.\n"
"DAVERAGE(A1:C7, \"Age\", A9:B11) bằng 36.\n"
"\n"
"@SEEALSO=DCOUNT"

