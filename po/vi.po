#: plugins/fn-string/functions.c:56
msgid ""
"@FUNCTION=CHAR\n"
"@SYNTAX=CHAR(x)\n"
"@DESCRIPTION=CHAR returns the ASCII character represented by the number @x.\n"
"@EXAMPLES=\n"
"CHAR(65) equals A.\n"
"\n"
"@SEEALSO=CODE"
msgstr ""
"@FUNCTION=CHAR\n"
"@SYNTAX=CHAR(x)\n"
"@DESCRIPTION=CHAR trả về ký tự ASCII đại diện cho số @x.\n"
"@EXAMPLES=\n"
"CHAR(65) bằng A.\n"
"\n"
"@SEEALSO=CODE"

#: plugins/fn-string/functions.c:82
msgid ""
"@FUNCTION=CODE\n"
"@SYNTAX=CODE(char)\n"
"@DESCRIPTION=CODE returns the ASCII number for the character @char.\n"
"This function is Excel compatible. \n"
"@EXAMPLES=\n"
"CODE(\"A\") equals 65.\n"
"\n"
"@SEEALSO=CHAR"
msgstr ""
"@FUNCTION=CODE\n"
"@SYNTAX=CODE(char)\n"
"@DESCRIPTION=CODE trả về mã ASCII của ký tự @char.\n"
"Hàm này tương thích với Excel.\n"
"@EXAMPLES=\n"
"CODE(\"A\") bằng 65.\n"
"\n"
"@SEEALSO=CHAR"

#: plugins/fn-string/functions.c:104
msgid ""
"@FUNCTION=EXACT\n"
"@SYNTAX=EXACT(string1, string2)\n"
"@DESCRIPTION=EXACT returns true if @string1 is exactly equal to @string2 "
"(this routine is case sensitive).\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"EXACT(\"key\",\"key\") equals TRUE.\n"
"EXACT(\"key\",\"Key\") equals FALSE.\n"
"\n"
"@SEEALSO=LEN, SEARCH, DELTA"
msgstr ""
"@FUNCTION=EXACT\n"
"@SYNTAX=EXACT(string1, string2)\n"
"@DESCRIPTION=EXACT trả về TRUE nếu @string1 trùng khớp hoàn toàn với "
"@string2 (hàm này phân biệt chữ hoa, chữ thường).\n"
"Hàm này tương thích với Excel.\n"
"@EXAMPLES=\n"
"EXACT(\"key\",\"key\") bằng TRUE.\n"
"EXACT(\"key\",\"Key\") bằng FALSE.\n"
"\n"
"@SEEALSO=LEN, SEARCH, DELTA"

#: plugins/fn-string/functions.c:129
msgid ""
"@FUNCTION=LEN\n"
"@SYNTAX=LEN(string)\n"
"@DESCRIPTION=LEN returns the length in characters of the string @string.\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"len(\"Helsinki\") equals 8.\n"
"\n"
"@SEEALSO=CHAR, CODE"
msgstr ""
"@FUNCTION=LEN\n"
"@SYNTAX=LEN(string)\n"
"@DESCRIPTION=LEN trả về độ dài (số ký tự) của chuỗi @string.\n"
"Hàm này tương thích với Excel.\n"
"@EXAMPLES=\n"
"len(\"Helsinki\") bằng 8.\n"
"\n"
"@SEEALSO=CHAR, CODE"

#: plugins/fn-string/functions.c:151
msgid ""
"@FUNCTION=LEFT\n"
"@SYNTAX=LEFT(text[,num_chars])\n"
"@DESCRIPTION=LEFT returns the leftmost @num_chars characters or the left "
"character if @num_chars is not specified.\n"
"This function is Excel compatible. \n"
"@EXAMPLES=\n"
"LEFT(\"Directory\",3) equals \"Dir\".\n"
"\n"
"@SEEALSO=MID, RIGHT"
msgstr ""
"@FUNCTION=LEFT\n"
"@SYNTAX=LEFT(text[,num_chars])\n"
"@DESCRIPTION=LEFT trả về @num_chars ký tự bên trái của chuỗi @text. Nếu "
"không có @num_chars thì @num_chars coi như bằng 1.\n"
"Hàm này tương thích với Excel. \n"
"@EXAMPLES=\n"
"LEFT(\"Directory\",3) bằng \"Dir\".\n"
"\n"
"@SEEALSO=MID, RIGHT"

#: plugins/fn-string/functions.c:187
msgid ""
"@FUNCTION=LOWER\n"
"@SYNTAX=LOWER(text)\n"
"@DESCRIPTION=LOWER returns a lower-case version of the string in @text.\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"LOWER(\"J. F. Kennedy\") equals \"j. f. kennedy\".\n"
"\n"
"@SEEALSO=UPPER"
msgstr ""
"@FUNCTION=LOWER\n"
"@SYNTAX=LOWER(text)\n"
"@DESCRIPTION=LOWER trả về chuỗi chữ thường của @text.\n"
"Hàm này tương thích với Excel.\n"
"@EXAMPLES=\n"
"LOWER(\"J. F. Kennedy\") bằng \"j. f. kennedy\".\n"
"\n"
"@SEEALSO=UPPER"

#: plugins/fn-string/functions.c:209
msgid ""
"@FUNCTION=MID\n"
"@SYNTAX=MID(string, position, length)\n"
"@DESCRIPTION=MID returns a substring from @string starting at @position for "
"@length characters.\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"MID(\"testing\",2,3) equals \"est\".\n"
"\n"
"@SEEALSO=LEFT, RIGHT"
msgstr ""
"@FUNCTION=MID\n"
"@SYNTAX=MID(string, position, length)\n"
"@DESCRIPTION=MID trả về chuỗi con bắt đầu từ @position, dài @length trong "
"chuỗi @string.\n"
"Hàm này tương thích với Excel.\n"
"@EXAMPLES=\n"
"MID(\"testing\",2,3) bằng \"est\".\n"
"\n"
"@SEEALSO=LEFT, RIGHT"

#: plugins/fn-string/functions.c:254
msgid ""
"@FUNCTION=RIGHT\n"
"@SYNTAX=RIGHT(text[,num_chars])\n"
"@DESCRIPTION=RIGHT returns the rightmost @num_chars characters or the right "
"character if @num_chars is not specified.\n"
"This function is Excel compatible. \n"
"@EXAMPLES=\n"
"RIGHT(\"end\") equals \"d\".\n"
"RIGHT(\"end\",2) equals \"nd\".\n"
"\n"
"@SEEALSO=MID, LEFT"
msgstr ""
"@FUNCTION=RIGHT\n"
"@SYNTAX=RIGHT(text[,num_chars])\n"
"@DESCRIPTION=RIGHT trả về @num_chars ký tự bên phải của chuỗi @text. "
"@num_chars mặc định bằng 1.\n"
"Hàm này tương thích với Excel. \n"
"@EXAMPLES=\n"
"RIGHT(\"end\") bằng \"d\".\n"
"RIGHT(\"end\",2) bằng \"nd\".\n"
"\n"
"@SEEALSO=MID, LEFT"

#: plugins/fn-string/functions.c:298
msgid ""
"@FUNCTION=UPPER\n"
"@SYNTAX=UPPER(text)\n"
"@DESCRIPTION=UPPER returns a upper-case version of the string in @text.\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"UPPER(\"canceled\") equals \"CANCELED\".\n"
"\n"
"@SEEALSO=LOWER"
msgstr ""
"@FUNCTION=UPPER\n"
"@SYNTAX=UPPER(text)\n"
"@DESCRIPTION=UPPER trả về chuỗi chữ hoa của @text.\n"
"Hàm này tương thích với Excel.\n"
"@EXAMPLES=\n"
"UPPER(\"canceled\") bằng \"CANCELED\".\n"
"\n"
"@SEEALSO=LOWER"

#: plugins/fn-string/functions.c:320
msgid ""
"@FUNCTION=CONCATENATE\n"
"@SYNTAX=CONCATENATE(string1[,string2...])\n"
"@DESCRIPTION=CONCATENATE returns up appended strings.\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"CONCATENATE(\"aa\",\"bb\") equals \"aabb\".\n"
"\n"
"@SEEALSO=LEFT, MID, RIGHT"
msgstr ""
"@FUNCTION=CONCATENATE\n"
"@SYNTAX=CONCATENATE(string1[,string2...])\n"
"@DESCRIPTION=CONCATENATE trả về chuỗi nối của các chuỗi đã cho.\n"
"Hàm này tương thích với Excel.\n"
"@EXAMPLES=\n"
"CONCATENATE(\"aa\",\"bb\") bằng \"aabb\".\n"
"\n"
"@SEEALSO=LEFT, MID, RIGHT"

#: plugins/fn-string/functions.c:344
msgid ""
"@FUNCTION=REPT\n"
"@SYNTAX=REPT(string,num)\n"
"@DESCRIPTION=REPT returns @num repetitions of @string.\n"
"This function is Excel compatible. \n"
"@EXAMPLES=\n"
"REPT(\".\",3) equals \"...\".\n"
"\n"
"@SEEALSO=CONCATENATE"
msgstr ""
"@FUNCTION=REPT\n"
"@SYNTAX=REPT(string,num)\n"
"@DESCRIPTION=REPT trả về chuỗi @string được lặp @num lần.\n"
"Hàm này tương thích với Excel.\n"
"@EXAMPLES=\n"
"REPT(\".\",3) bằng \"...\".\n"
"\n"
"@SEEALSO=CONCATENATE"

#: plugins/fn-string/functions.c:390
msgid ""
"@FUNCTION=CLEAN\n"
"@SYNTAX=CLEAN(string)\n"
"@DESCRIPTION=CLEAN removes any non-printable characters from @string.\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"CLEAN(\"one\"\\&char(7)) equals \"one\".\n"
"\n"
"@SEEALSO="
msgstr ""
"@FUNCTION=CLEAN\n"
"@SYNTAX=CLEAN(string)\n"
"@DESCRIPTION=CLEAN loại bỏ các ký tự không in được ra khỏi @string.\n"
"Hàm này tương thích với Excel.\n"
"@EXAMPLES=\n"
"CLEAN(\"one\"\\&char(7)) bằng \"one\".\n"
"\n"
"@SEEALSO="

#: plugins/fn-string/functions.c:423
msgid ""
"@FUNCTION=FIND\n"
"@SYNTAX=FIND(string1,string2[,start])\n"
"@DESCRIPTION=FIND returns position of @string1 in @string2 (case-sensitive), "
"searching only from character @start onwards (assuming 1 if omitted).\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"FIND(\"ac\",\"Jack\") equals 2.\n"
"\n"
"@SEEALSO=EXACT, LEN, MID, SEARCH"
msgstr ""
"@FUNCTION=FIND\n"
"@SYNTAX=FIND(string1,string2[,start])\n"
"@DESCRIPTION=FIND trả về vị trí của chuỗi @string1 trong @string2 (phân biệt "
"chữ hoa, chữ thường). Tìm kiếm bắt đầu từ @start (nếu có).\n"
"Hàm này tương thích với Excel.\n"
"@EXAMPLES=\n"
"FIND(\"ac\",\"Jack\") bằng 2.\n"
"\n"
"@SEEALSO=EXACT, LEN, MID, SEARCH"

#: plugins/fn-string/functions.c:478
msgid ""
"@FUNCTION=FIXED\n"
"@SYNTAX=FIXED(num,[decimals, no_commas])\n"
"@DESCRIPTION=FIXED returns @num as a formatted string with @decimals numbers "
"after the decimal point, omitting commas if requested by @no_commas.\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"FIXED(1234.567,2) equals \"1,234.57\".\n"
"\n"
"@SEEALSO="
msgstr ""

#: plugins/fn-string/functions.c:546
msgid ""
"@FUNCTION=PROPER\n"
"@SYNTAX=PROPER(string)\n"
"@DESCRIPTION=PROPER returns @string with initial of each word capitalised.\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"PROPER(\"j. f. kennedy\") equals \"J. F. Kennedy\".\n"
"\n"
"@SEEALSO=LOWER, UPPER"
msgstr ""
"@FUNCTION=PROPER\n"
"@SYNTAX=PROPER(string)\n"
"@DESCRIPTION=PROPER trả về chuỗi được viết hoa ký tự đầu tiên.\n"
"Hàm này tương thích với Excel.\n"
"@EXAMPLES=\n"
"PROPER(\"j. f. kennedy\") bằng \"J. F. Kennedy\".\n"
"\n"
"@SEEALSO=LOWER, UPPER"

#: plugins/fn-string/functions.c:596
msgid ""
"@FUNCTION=REPLACE\n"
"@SYNTAX=REPLACE(old,start,num,new)\n"
"@DESCRIPTION=REPLACE returns @old with @new replacing @num characters from "
"@start.\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"REPLACE(\"testing\",2,3,\"*****\") equals \"t*****ing\".\n"
"\n"
"@SEEALSO=MID, SEARCH, SUBSTITUTE, TRIM"
msgstr ""
"@FUNCTION=REPLACE\n"
"@SYNTAX=REPLACE(old,start,num,new)\n"
"@DESCRIPTION=REPLACE trả về chuỗi @old được thay thế bằng chuỗi @start từ vị "
"trí @new, dài @num ký tự.\n"
"Hàm này tương thích với Excel.\n"
"@EXAMPLES=\n"
"REPLACE(\"testing\",2,3,\"*****\") bằng \"t*****ing\".\n"
"\n"
"@SEEALSO=MID, SEARCH, SUBSTITUTE, TRIM"

#: plugins/fn-string/functions.c:625
msgid "Arguments out of range"
msgstr "Đối số nằm ngoài vùng cho phép"

#: plugins/fn-string/functions.c:645
msgid ""
"@FUNCTION=T\n"
"@SYNTAX=T(value)\n"
"@DESCRIPTION=T returns @value if and only if it is text, otherwise a blank "
"string.\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"T(\"text\") equals \"text\".\n"
"T(64) returns an empty cell.\n"
"\n"
"@SEEALSO=CELL, N, VALUE"
msgstr ""

#: plugins/fn-string/functions.c:673
msgid ""
"@FUNCTION=TEXT\n"
"@SYNTAX=TEXT(value,format_text)\n"
"@DESCRIPTION=TEXT returns @value as a string with the specified format.\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"TEXT(3.223,\"$0.00\") equals \"$3.22\".\n"
"TEXT(date(1999,4,15),\"mmmm, dd, yy\") equals \"April, 15, 99\".\n"
"\n"
"@SEEALSO=DOLLAR"
msgstr ""

#: plugins/fn-string/functions.c:709
msgid "Type mismatch"
msgstr "Sai kiểu"

#: plugins/fn-string/functions.c:721
msgid ""
"@FUNCTION=TRIM\n"
"@SYNTAX=TRIM(text)\n"
"@DESCRIPTION=TRIM returns @text with only single spaces between words.\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"TRIM(\"  a bbb  cc\") equals \"a bbb cc\".\n"
"\n"
"@SEEALSO=CLEAN, MID, REPLACE, SUBSTITUTE"
msgstr ""
"@FUNCTION=TRIM\n"
"@SYNTAX=TRIM(text)\n"
"@DESCRIPTION=TRIM trả về chuỗi @text chỉ có một khoảng trắng giữa các từ.\n"
"Hàm này tương thích với Excel.\n"
"@EXAMPLES=\n"
"TRIM(\"  a bbb  cc\") bằng \"a bbb cc\".\n"
"\n"
"@SEEALSO=CLEAN, MID, REPLACE, SUBSTITUTE"

#: plugins/fn-string/functions.c:773
msgid ""
"@FUNCTION=VALUE\n"
"@SYNTAX=VALUE(text)\n"
"@DESCRIPTION=VALUE returns numeric value of @text.\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"VALUE(\"$1,000\") equals 1000.\n"
"\n"
"@SEEALSO=DOLLAR, FIXED, TEXT"
msgstr ""
"@FUNCTION=VALUE\n"
"@SYNTAX=VALUE(text)\n"
"@DESCRIPTION=VALUE trả về giá trị số của @text.\n"
"Hàm này tương thích với Excel.\n"
"@EXAMPLES=\n"
"VALUE(\"$1,000\") bằng 1000.\n"
"\n"
"@SEEALSO=DOLLAR, FIXED, TEXT"

#: plugins/fn-string/functions.c:815
msgid ""
"@FUNCTION=SUBSTITUTE\n"
"@SYNTAX=SUBSTITUTE(text, old, new [,num])\n"
"@DESCRIPTION=SUBSTITUTE replaces @old with @new in @text.  Substitutions are "
"only applied to instance @num of @old in @text, otherwise every one is "
"changed.\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"SUBSTITUTE(\"testing\",\"test\",\"wait\") equals \"waiting\".\n"
"\n"
"@SEEALSO=REPLACE, TRIM"
msgstr ""

#: plugins/fn-string/functions.c:931
msgid ""
"@FUNCTION=DOLLAR\n"
"@SYNTAX=DOLLAR(num[,decimals])\n"
"@DESCRIPTION=DOLLAR returns @num formatted as currency.\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"DOLLAR(12345) equals \"$12,345.00\".\n"
"\n"
"@SEEALSO=FIXED, TEXT, VALUE"
msgstr ""
"@FUNCTION=DOLLAR\n"
"@SYNTAX=DOLLAR(num[,decimals])\n"
"@DESCRIPTION=DOLLAR trả về @num trong dạng thức tiền tệ.\n"
"Hàm này tương thích với Excel.\n"
"@EXAMPLES=\n"
"DOLLAR(12345) bằng \"$12,345.00\".\n"
"\n"
"@SEEALSO=FIXED, TEXT, VALUE"

#: plugins/fn-string/functions.c:1008
msgid ""
"@FUNCTION=SEARCH\n"
"@SYNTAX=SEARCH(text,within[,start_num])\n"
"@DESCRIPTION=SEARCH returns the location of a character or text string "
"within another string.  @text is the string or character to be searched. "
"@within is the string in which you want to search.  @start_num is the start "
"position of the search in @within.  If @start_num is omitted, it is assumed "
"to be one.  The search is not case sensitive. \n"
"@text can contain wildcard characters (*) and question marks (?) to control "
"the search.  A question mark matches with any character and wildcard matches "
"with any string including empty string.  If you want the actual wildcard or "
"question mark to be searched, use tilde (~) before the character. \n"
"If @text is not found, SEARCH returns #VALUE! error. If @start_num is less "
"than one or it is greater than the length of @within, SEARCH returns #VALUE! "
"error.\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"SEARCH(\"c\",\"Cancel\") equals 1.\n"
"SEARCH(\"c\",\"Cancel\",2) equals 4.\n"
"\n"
"@SEEALSO=FIND"
msgstr ""

