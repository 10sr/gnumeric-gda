#: plugins/numtheory/numtheory.c:198
msgid ""
"@FUNCTION=NT_PHI\n"
"@SYNTAX=NT_PHI(n)\n"
"@DESCRIPTION=The NT_PHI function calculates the number of integers less than "
"or equal to @n that are relatively prime to @n.\n"
"This function only takes one argument.\n"
"@EXAMPLES=\n"
"@SEEALSO=NT_D, ITHPRIME, NT_SIGMA"
msgstr ""
"@FUNCTION=NT_PHI\n"
"@SYNTAX=NT_PHI(n)\n"
"@DESCRIPTION=Hàm NT_PHI tính số nguyên nhỏ hơn hoặc bằng @n mà tương ứng với số nguyên tố thứ @n.\n"
"Hàm này chỉ có một đối số.\n"
"@EXAMPLES=\n"
"@SEEALSO=NT_D, ITHPRIME, NT_SIGMA"

#: plugins/numtheory/numtheory.c:233
msgid ""
"@FUNCTION=NT_MU\n"
"@SYNTAX=NT_MU(n)\n"
"@DESCRIPTION=The NT_MU function (Mbius mu function) returns \n"
"0  if @n is divisible by the square of a prime .\n"
"Otherwise it returns: \n"
"-1 if @n has an odd  number of different prime factors .\n"
"1  if @n has an even number of different prime factors .\n"
"If @n=1 it returns 1\n"
"\n"
"@EXAMPLES=\n"
"@SEEALSO=NT_D, ITHPRIME, NT_PHI"
msgstr ""
"@FUNCTION=NT_MU\n"
"@SYNTAX=NT_MU(n)\n"
"@DESCRIPTION=Hàm NT_MU (Hàm Mbius mu) trả về\n"
"0  nếu @n là có thể divide by the square of a prime.\n"
"Nếu khác nó trả về:\n"
"-1 nếu @n là số lẻ của prime factor khác.\n"
"1  nếu @n là số chẵn của prime factors khác.\n"
"Nếu @n=1 thì trả về 1\n"
"\n"
"@EXAMPLES=\n"
"@SEEALSO=NT_D, ITHPRIME, NT_PHI"

#: plugins/numtheory/numtheory.c:272
msgid ""
"@FUNCTION=NT_D\n"
"@SYNTAX=NT_D(n)\n"
"@DESCRIPTION=The NT_D function calculates the number of divisors of @n.\n"
"This function only takes one argument.\n"
"@EXAMPLES=\n"
"@SEEALSO=ITHPRIME, NT_PHI, NT_SIGMA"
msgstr ""
"@FUNCTION=NT_D\n"
"@SYNTAX=NT_D(n)\n"
"@DESCRIPTION=Hàm NT_D tính divisors của @n.\n"
"Hàm này chỉ nhận một đối số.\n"
"@EXAMPLES=\n"
"@SEEALSO=ITHPRIME, NT_PHI, NT_SIGMA"

#: plugins/numtheory/numtheory.c:306
msgid ""
"@FUNCTION=NT_SIGMA\n"
"@SYNTAX=NT_SIGMA(n)\n"
"@DESCRIPTION=The NT_SIGMA function calculates the sum of the divisors of "
"@n.\n"
"This function only takes one argument.\n"
"@EXAMPLES=\n"
"@SEEALSO=NT_D, ITHPRIME, NT_PHI"
msgstr ""
"@FUNCTION=NT_SIGMA\n"
"@SYNTAX=NT_SIGMA(n)\n"
"@DESCRIPTION=Hàm NT_SIGMA tính tổng divisor của @n.\n"
"Hàm này chỉ nhận một đối số.\n"
"@EXAMPLES=\n"
"@SEEALSO=NT_D, ITHPRIME, NT_PHI"

#: plugins/numtheory/numtheory.c:341
msgid ""
"@FUNCTION=ITHPRIME\n"
"@SYNTAX=ITHPRIME(i)\n"
"@DESCRIPTION=The ITHPRIME function returns the @ith prime.\n"
"This function only takes one argument.\n"
"@EXAMPLES=\n"
"@SEEALSO=NT_D, NT_SIGMA"
msgstr ""
"@FUNCTION=ITHPRIME\n"
"@SYNTAX=ITHPRIME(i)\n"
"@DESCRIPTION=Hàm ITHPRIME trả về số nguyên tố thứ @i.\n"
"Hàm này chỉ nhận một đối số.\n"
"@EXAMPLES=\n"
"@SEEALSO=NT_D, NT_SIGMA"

#: plugins/numtheory/numtheory.c:369
msgid ""
"@FUNCTION=ISPRIME\n"
"@SYNTAX=ISPRIME(i)\n"
"@DESCRIPTION=The ISPRIME function returns TRUE if @i is prime and FALSE "
"otherwise.\n"
"This function only takes one argument.\n"
"@SEEALSO=ITHPRIME, NT_D, NT_SIGMA"
msgstr ""
"@FUNCTION=ISPRIME\n"
"@SYNTAX=ISPRIME(i)\n"
"@DESCRIPTION=Hàm ISPRIME trả về ĐÚNG nếu @i là số nguyên tố, SAI nếu không phải.\n"
"Hàm này chỉ nhận một đối số.\n"
"@SEEALSO=ITHPRIME, NT_D, NT_SIGMA"

#: plugins/numtheory/numtheory.c:395
msgid ""
"@FUNCTION=NT_PI\n"
"@SYNTAX=NT_PI(n)\n"
"@DESCRIPTION=The NT_PI function returns the number of primes less than or "
"equal to @n.\n"
"This function only takes one argument.\n"
"@SEEALSO=ITHPRIME, NT_PHI, NT_D, NT_SIGMA"
msgstr ""
"@FUNCTION=NT_PI\n"
"@SYNTAX=NT_PI(n)\n"
"@DESCRIPTION=Hàm NT_PI trả về số số nguyên tố nhỏ hơn hay bằng @n.\n"
"Hàm này chỉ nhận một đối số.\n"
"@SEEALSO=ITHPRIME, NT_PHI, NT_D, NT_SIGMA"

