#: plugins/numtheory/numtheory.c:212
msgid ""
"@FUNCTION=NT_PHI\n"
"@SYNTAX=NT_PHI(n)\n"
"@DESCRIPTION=The NT_PHI function calculates the number of integers less than "
"or equal to @n that are relatively prime to @n.\n"
"This function only takes one argument.\n"
"@EXAMPLES=\n"
"@SEEALSO=NT_D, ITHPRIME, NT_SIGMA"
msgstr ""
"@FUNCTION=NT_PHI\n"
"@SYNTAX=NT_PHI(n)\n"
"@DESCRIPTION=Hàm NT_PHI tính số nguyên nhỏ hơn hoặc bằng @n mà tương ứng với "
"số nguyên tố thứ @n.\n"
"Hàm này chỉ có một đối số.\n"
"@EXAMPLES=\n"
"@SEEALSO=NT_D, ITHPRIME, NT_SIGMA"

#: plugins/numtheory/numtheory.c:247
msgid ""
"@FUNCTION=NT_MU\n"
"@SYNTAX=NT_MU(n)\n"
"@DESCRIPTION=The NT_MU function (Möbius mu function) returns \n"
"0  if @n is divisible by the square of a prime .\n"
"Otherwise it returns: \n"
"-1 if @n has an odd  number of different prime factors .\n"
"1  if @n has an even number of different prime factors .\n"
"If @n=1 it returns 1\n"
"\n"
"@EXAMPLES=\n"
"@SEEALSO=NT_D, ITHPRIME, NT_PHI"
msgstr ""
"@FUNCTION=NT_MU\n"
"@SYNTAX=NT_MU(n)\n"
"@DESCRIPTION=Hàm NT_MU (Hàm Mbius mu) trả về\n"
"0  nếu @n là có thể chia hết cho bình phương của một số nguyên tố.\n"
"Nếu khác nó trả về:\n"
"-1 nếu tổng các thừa số nguyên tố mà tích của chúng tạo thành @n là một số "
"lẻ.\n"
"1  nếu tổng các thừa số nguyên tố mà tích của chúng tạo thành @n là một số "
"chẵn.\n"
"Nếu @n=1 thì trả về 1\n"
"\n"
"@EXAMPLES=\n"
"@SEEALSO=NT_D, ITHPRIME, NT_PHI"

#: plugins/numtheory/numtheory.c:286
msgid ""
"@FUNCTION=NT_D\n"
"@SYNTAX=NT_D(n)\n"
"@DESCRIPTION=The NT_D function calculates the number of divisors of @n.\n"
"This function only takes one argument.\n"
"@EXAMPLES=\n"
"@SEEALSO=ITHPRIME, NT_PHI, NT_SIGMA"
msgstr ""
"@FUNCTION=NT_D\n"
"@SYNTAX=NT_D(n)\n"
"@DESCRIPTION=Hàm NT_D tính divisors của @n.\n"
"Hàm này chỉ nhận một đối số.\n"
"@EXAMPLES=\n"
"@SEEALSO=ITHPRIME, NT_PHI, NT_SIGMA"

#: plugins/numtheory/numtheory.c:320
msgid ""
"@FUNCTION=NT_SIGMA\n"
"@SYNTAX=NT_SIGMA(n)\n"
"@DESCRIPTION=The NT_SIGMA function calculates the sum of the divisors of "
"@n.\n"
"This function only takes one argument.\n"
"@EXAMPLES=\n"
"@SEEALSO=NT_D, ITHPRIME, NT_PHI"
msgstr ""
"@FUNCTION=NT_SIGMA\n"
"@SYNTAX=NT_SIGMA(n)\n"
"@DESCRIPTION=Hàm NT_SIGMA tính tổng divisor của @n.\n"
"Hàm này chỉ nhận một đối số.\n"
"@EXAMPLES=\n"
"@SEEALSO=NT_D, ITHPRIME, NT_PHI"

#: plugins/numtheory/numtheory.c:355
msgid ""
"@FUNCTION=ITHPRIME\n"
"@SYNTAX=ITHPRIME(i)\n"
"@DESCRIPTION=The ITHPRIME function returns the @ith prime.\n"
"This function only takes one argument.\n"
"@EXAMPLES=\n"
"@SEEALSO=NT_D, NT_SIGMA"
msgstr ""
"@FUNCTION=ITHPRIME\n"
"@SYNTAX=ITHPRIME(i)\n"
"@DESCRIPTION=Hàm ITHPRIME trả về số nguyên tố thứ @i.\n"
"Hàm này chỉ nhận một đối số.\n"
"@EXAMPLES=\n"
"@SEEALSO=NT_D, NT_SIGMA"

#: plugins/numtheory/numtheory.c:383
msgid ""
"@FUNCTION=ISPRIME\n"
"@SYNTAX=ISPRIME(i)\n"
"@DESCRIPTION=The ISPRIME function returns TRUE if @i is prime and FALSE "
"otherwise.\n"
"This function only takes one argument.\n"
"@SEEALSO=ITHPRIME, NT_D, NT_SIGMA"
msgstr ""
"@FUNCTION=ISPRIME\n"
"@SYNTAX=ISPRIME(i)\n"
"@DESCRIPTION=Hàm ISPRIME trả về TRUE nếu @i là số nguyên tố, FALSE nếu không "
"phải.\n"
"Hàm này chỉ nhận một đối số.\n"
"@SEEALSO=ITHPRIME, NT_D, NT_SIGMA"

#: plugins/numtheory/numtheory.c:409
msgid ""
"@FUNCTION=NT_PI\n"
"@SYNTAX=NT_PI(n)\n"
"@DESCRIPTION=The NT_PI function returns the number of primes less than or "
"equal to @n.\n"
"This function only takes one argument.\n"
"@SEEALSO=ITHPRIME, NT_PHI, NT_D, NT_SIGMA"
msgstr ""
"@FUNCTION=NT_PI\n"
"@SYNTAX=NT_PI(n)\n"
"@DESCRIPTION=Hàm NT_PI trả về số số nguyên tố nhỏ hơn hay bằng @n.\n"
"Hàm này chỉ nhận một đối số.\n"
"@SEEALSO=ITHPRIME, NT_PHI, NT_D, NT_SIGMA"

#: plugins/numtheory/numtheory.c:436
msgid ""
"@FUNCTION=BITOR\n"
"@SYNTAX=BITOR(a,b)\n"
"@DESCRIPTION=The BITOR function returns bitwise or-ing of its arguments.\n"
"\n"
"@EXAMPLES=\n"
"@SEEALSO=BITXOR,BITAND"
msgstr ""
"@FUNCTION=BITOR\n"
"@SYNTAX=BITOR(a,b)\n"
"@DESCRIPTION=Hàm BITOR thực hiện toán tử OR theo bit đối với các đối số.\n"
"\n"
"@EXAMPLES=\n"
"@SEEALSO=BITXOR,BITAND"

#: plugins/numtheory/numtheory.c:455
msgid ""
"@FUNCTION=BITXOR\n"
"@SYNTAX=BITXOR(a,b)\n"
"@DESCRIPTION=The BITXOR function returns bitwise exclusive or-ing of its "
"arguments.\n"
"\n"
"@EXAMPLES=\n"
"@SEEALSO=BITOR,BITAND"
msgstr ""
"@FUNCTION=BITXOR\n"
"@SYNTAX=BITXOR(a,b)\n"
"@DESCRIPTION=Hàm BITXOR thực hiện XOR theo bit các đối số.\n"
"\n"
"@EXAMPLES=\n"
"@SEEALSO=BITOR,BITAND"

#: plugins/numtheory/numtheory.c:474
msgid ""
"@FUNCTION=BITAND\n"
"@SYNTAX=BITAND(a,b)\n"
"@DESCRIPTION=The BITAND function returns bitwise and-ing of its arguments.\n"
"\n"
"@EXAMPLES=\n"
"@SEEALSO=BITOR,BITXOR"
msgstr ""
"@FUNCTION=BITAND\n"
"@SYNTAX=BITAND(a,b)\n"
"@DESCRIPTION=Hàm BITAND thực hiện AND theo bit các đối số.\n"
"\n"
"@EXAMPLES=\n"
"@SEEALSO=BITOR,BITXOR"

#: plugins/numtheory/numtheory.c:494
msgid ""
"@FUNCTION=BITLSHIFT\n"
"@SYNTAX=BITLSHIFT(x,n)\n"
"@DESCRIPTION=The BITLSHIFT function returns @x bit-shifted left by @n bits.\n"
"If @n is negative, a right shift will in effect be performed.\n"
"\n"
"@EXAMPLES=\n"
"@SEEALSO=BITRSHIFT"
msgstr ""
"@FUNCTION=BITLSHIFT\n"
"@SYNTAX=BITLSHIFT(x,n)\n"
"@DESCRIPTION=Hàm BITLSHIFT thực hiện phép dịch trái @n lần đối với @x.\n"
"Nếu @n âm thì thực hiện dịch phải -@n lần.\n"
"\n"
"@EXAMPLES=\n"
"@SEEALSO=BITRSHIFT"

#: plugins/numtheory/numtheory.c:521
msgid ""
"@FUNCTION=BITRSHIFT\n"
"@SYNTAX=BITRSHIFT(x,n)\n"
"@DESCRIPTION=The BITRSHIFT function returns @x bit-shifted right by @n "
"bits.\n"
"If @n is negative, a left shift will in effect be performed.\n"
"\n"
"@EXAMPLES=\n"
"@SEEALSO=BITLSHIFT"
msgstr ""
"@FUNCTION=BITRSHIFT\n"
"@SYNTAX=BITRSHIFT(x,n)\n"
"@DESCRIPTION=Hàm BITRSHIFT thực hiện dịch phải @n lần đối với @x.\n"
"Nếu @n âm thì thực hiện dịch trái -@n lần.\n"
"\n"
"@EXAMPLES=\n"
"@SEEALSO=BITLSHIFT"

