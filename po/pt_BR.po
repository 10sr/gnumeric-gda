#: src/functions/fn-stat.c:4708
msgid "Statistics"
msgstr "Estatísticas"

# src/fn-string.c:17
#: src/functions/fn-string.c:23
msgid ""
"@FUNCTION=CHAR\n"
"@SYNTAX=CHAR(x)\n"
"@DESCRIPTION=CHAR returns the ASCII character represented by the number @x.\n"
"@EXAMPLES=\n"
"CHAR(65) equals A.\n"
"\n"
"@SEEALSO=CODE"
msgstr ""
"@FUNCTION=CHAR\n"
"@SYNTAX=CHAR(x)\n"
"@DESCRIPTION=Retorna o caractere ASCII representado pelo número @x.\n"
"@EXAMPLES=\n"
"CHAR(65) igual a A.\n"
"\n"
"@SEEALSO=CODE"

# src/fn-string.c:39
#: src/functions/fn-string.c:49
msgid ""
"@FUNCTION=CODE\n"
"@SYNTAX=CODE(char)\n"
"@DESCRIPTION=CODE returns the ASCII number for the character @char.\n"
"@EXAMPLES=\n"
"CODE(\"A\") equals 65.\n"
"\n"
"@SEEALSO=CHAR"
msgstr ""
"@FUNCTION=CODE\n"
"@SYNTAX=CODE(caractere)\n"
"@DESCRIPTION=Retorna o número ASCII para o caractere @char.\n"
"@EXAMPLES=\n"
"CODE(\"A\") igual a 65.\n"
"\n"
"@SEEALSO=CHAR"

# src/fn-string.c:61
#: src/functions/fn-string.c:76
msgid ""
"@FUNCTION=EXACT\n"
"@SYNTAX=EXACT(string1, string2)\n"
"@DESCRIPTION=EXACT returns true if @string1 is exactly equal to @string2 "
"(this routine is case sensitive).\n"
"@EXAMPLES=\n"
"EXACT(\"key\",\"key\") equals TRUE.\n"
"EXACT(\"key\",\"Key\") equals FALSE.\n"
"\n"
"@SEEALSO=LEN"
msgstr ""
"@FUNCTION=EXACT\n"
"@SYNTAX=EXACT(cadeia1, cadeia2)\n"
"@DESCRIPTION=EXACT retorna verdadeiro se a @cadeia1 for exatamente igual a "
"@cadeia2(esta rotina é sensível a maiúsculas/minúsculas).\n"
"@EXAMPLES=\n"
"EXACT(\"key\",\"key\") igual a VERDADEIRO.\n"
"EXACT(\"key\",\"Key\") igual a FALSO.\n"
"\n"
"@SEEALSO=LEN"

# src/fn-string.c:83
#: src/functions/fn-string.c:103
msgid ""
"@FUNCTION=LEN\n"
"@SYNTAX=LEN(string)\n"
"@DESCRIPTION=LEN returns the length in characters of the string @string.\n"
"@EXAMPLES=\n"
"len(\"Helsinki\") equals 8.\n"
"\n"
"@SEEALSO=CHAR, CODE"
msgstr ""
"@FUNCTION=LEN\n"
"@SYNTAX=LEN(cadeia)\n"
"@DESCRIPTION=LEN retorna a extensão em caracteres da cadeia @cadeia..\n"
"@EXAMPLES=\n"
"len(\"Helsinki\") igual a 8.\n"
"\n"
"@SEEALSO=CHAR, CODE"

# src/fn-string.c:140
#: src/functions/fn-string.c:127
msgid ""
"@FUNCTION=LEFT\n"
"@SYNTAX=LEFT(text[,num_chars])\n"
"@DESCRIPTION=LEFT returns the leftmost @num_chars characters or the left "
"character if @num_chars is not specified.\n"
"@EXAMPLES=\n"
"LEFT(\"Directory\",3) equals \"Dir\".\n"
"\n"
"@SEEALSO=MID, RIGHT"
msgstr ""
"@FUNCTION=LEFT\n"
"@SYNTAX=LEFT(texto[,num_caracteres])\n"
"@DESCRIPTION=LEFT retorna os caracteres @num_caracteres mais à esquerda ouo "
"caracter esquerdo se @num_chars não for especificado.\n"
"@EXAMPLES=\n"
"LEFT(\"Directory\",3) equals \"Dir\".\n"
"\n"
"@SEEALSO=MID, RIGHT"

# src/fn-string.c:176
#: src/functions/fn-string.c:165
msgid ""
"@FUNCTION=LOWER\n"
"@SYNTAX=LOWER(text)\n"
"@DESCRIPTION=LOWER returns a lower-case version of the string in @text.\n"
"@EXAMPLES=\n"
"LOWER(\"J. F. Kennedy\") equals \"j. f. kennedy\".\n"
"\n"
"@SEEALSO=UPPER"
msgstr ""
"@FUNCTION=LOWER\n"
"@SYNTAX=LOWER(texto)\n"
"@DESCRIPTION=LOWER retorna uma versão de minúscula da cadeia no @texto.\n"
"@EXAMPLES=\n"
"LOWER(\"J. F. Kennedy\") igual a \"j. f. kennedy\".\n"
"\n"
"@SEEALSO=UPPER"

# src/fn-string.c:209
#: src/functions/fn-string.c:199
msgid ""
"@FUNCTION=MID\n"
"@SYNTAX=MID(string, position, length)\n"
"@DESCRIPTION=MID returns a substring from @string starting at @position for "
"@length characters.\n"
"@EXAMPLES=\n"
"MID(\"testing\",2,3) equals \"est\".\n"
"\n"
"@SEEALSO=LEFT, RIGHT"
msgstr ""
"@FUNCTION=MID\n"
"@SYNTAX=MID(cadeia, posição, extensão)\n"
"@DESCRIPTION=MID retorna uma subcadeia da @cadeia começando na @posição para "
"uma @extensão de caracteres.\n"
"@EXAMPLES=\n"
"MID(\"testing\",2<sect3><title></title></sect3>) igual \"est\".\n"
"\n"
"@SEEALSO=LEFT, RIGHT"

# plugins/guile/plugin.c:241 src/expr.c:489 src/fn-math.c:1177
# src/fn-sheet.c:38 src/fn-sheet.c:83
#: src/functions/fn-string.c:228 src/functions/fn-string.c:681
msgid "Invalid arguments"
msgstr "Argumentos inválidos"

# src/fn-string.c:257
#: src/functions/fn-string.c:246
msgid ""
"@FUNCTION=RIGHT\n"
"@SYNTAX=RIGHT(text[,num_chars])\n"
"@DESCRIPTION=RIGHT returns the rightmost @num_chars characters or the right "
"character if @num_chars is not specified.\n"
"@EXAMPLES=\n"
"RIGHT(\"end\") equals \"d\".\n"
"RIGHT(\"end\",2) equals \"nd\".\n"
"\n"
"@SEEALSO=MID, LEFT"
msgstr ""
"@FUNCTION=RIGHT\n"
"@SYNTAX=RIGHT(texto[,nr_caracteres])\n"
"@DESCRIPTION=Retorna os [nr_caracteres] mais à direita, ou o caractere mais "
"à direita se nr_caracteres não for especificado.\n"
"@EXAMPLES=\n"
"RIGHT(\"end\") equals \"d\".\n"
"RIGHT(\"end\",2) equals \"nd\".\n"
"\n"
"@SEEALSO=MID, LEFT"

# src/fn-string.c:297
#: src/functions/fn-string.c:289
msgid ""
"@FUNCTION=UPPER\n"
"@SYNTAX=UPPER(text)\n"
"@DESCRIPTION=UPPER returns a upper-case version of the string in @text.\n"
"@EXAMPLES=\n"
"UPPER(\"canceled\") equals \"CANCELED\".\n"
"\n"
"@SEEALSO=LOWER"
msgstr ""
"@FUNCTION=UPPER\n"
"@SYNTAX=UPPER(texto)\n"
"@DESCRIPTION= retorna uma versão em maiúsculas da expressão no @texto\n"
"@EXAMPLES=\n"
"UPPER(\"canceled\") igual a \"CANCELED\".\n"
"\n"
"@SEEALSO=LOWER"

#: src/functions/fn-string.c:324
msgid ""
"@FUNCTION=CONCATENATE\n"
"@SYNTAX=CONCATENATE(string1[,string2...])\n"
"@DESCRIPTION=CONCATENATE returns up appended strings.\n"
"@EXAMPLES=\n"
"CONCATENATE(\"aa\",\"bb\") equals \"aabb\".\n"
"\n"
"@SEEALSO=LEFT, MID, RIGHT"
msgstr ""
"@FUNCTION=CONCATENATE\n"
"@SYNTAX=CONCATENATE(cadeia1[,cadeia2...])\n"
"@DESCRIPTION=CONCATENATE retorna cadeias anexadas.\n"
"@EXAMPLES=\n"
"CONCATENATE(\"aa\",\"bb\") igual a \"aabb\".\n"
"\n"
"@SEEALSO=LEFT, MID, RIGHT"

# src/fn-string.c:83
#: src/functions/fn-string.c:376
msgid ""
"@FUNCTION=REPT\n"
"@SYNTAX=REPT(string,num)\n"
"@DESCRIPTION=REPT returns @num repetitions of @string.\n"
"@EXAMPLES=\n"
"REPT(\".\",3) equals \"...\".\n"
"\n"
"@SEEALSO=CONCATENATE"
msgstr ""
"@FUNCTION=REPT\n"
"@SYNTAX=REPT(cadeia,num)\n"
"@DESCRIPTION=REPT retorna repetições @num de @cadeia.\n"
"@EXAMPLES=\n"
"REPT(\".\",3) igual a \"...\".\n"
"\n"
"@SEEALSO=CONCATENATE"

