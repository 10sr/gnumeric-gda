"hata ile sonuçlanýr:\n"
"    ATS     (Avusturya)\n"
"    BEF     (Belçika)\n"
"    DEM     (Almanya)\n"
"    ESP     (Ýspanya)\n"
"    FIM     (Finlandiya)\n"
"    FRF     (Fransa)\n"
"    IEP     (Ýrlanda)\n"
"    ITL     (Ýtalya)\n"
"    LUF     (Luxemburg)\n"
"    NLG     (Hollanda)\n"
