prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@

# To retrieve the module from anonymous CVS, use the following command:
#   cvs -z4 -d:pserver:anonymous@anoncvs.gnome.org:/cvs/gnome co gnumeric

Name: libspreadsheet
Description: The spreadsheet internals of Gnumeric (DEVELOPMENT VERSION)
Version: @VERSION@
Requires: glib-2.0 gobject-2.0 libgsf-1 libxml-2.0 gtk+-2.0 libglade-2.0 libgnomeprint-2.2 libart-2.0 goffice-0.3
Libs: -L${libdir} -lgoffice-0
Libs.private: @EXTRA_LIBS@
Cflags: -I${includedir}/libspreadsheet-1-7 @EXTRA_INCLUDES@
GOffice_plugins_dir=${gnumeric_plugindir}
