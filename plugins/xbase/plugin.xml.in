<?xml version="1.0"?>
<plugin id="Gnumeric_xbase">
	<information>
		<_name>XBase</_name>
		<_description>Imports XBase files</_description>
	</information>
	<loader type="Gnumeric_Builtin:module">
		<attribute name="module_file" value="xbase.la"/>
	</loader>
	<services>
		<service type="file_opener" id="xbase" priority="100" probe="FALSE"
			open="TRUE" import="TRUE">
			<information>
				<_description>Xbase (*.dbf) file format</_description>
			</information>
			<file_patterns>
				<file_pattern type="shell_pattern" value="*.dbf"/>
			</file_patterns>
		</service>
	</services>
</plugin>
