<?xml version="1.0"?>
<plugin id="Gnumeric_oleo">
	<information>
		<_name>GNU Oleo</_name>
		<_description>Imports GNU Oleo documents</_description>
	</information>
	<loader type="Gnumeric_Builtin:module">
		<attribute name="module_file" value="oleo.la"/>
	</loader>
	<services>
		<service type="file_opener" id="oleo" priority="100" probe="FALSE">
			<information>
				<_description>GNU Oleo (*.oleo)</_description>
			</information>
			<file_patterns>
				<file_pattern type="shell_pattern" value="*.oleo"/>
			</file_patterns>
		</service>
	</services>
</plugin>
