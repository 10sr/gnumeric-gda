<?xml version="1.0"?>
<plugin id="Gnumeric_psiconv">
	<information>
		<_name>Psiconv</_name>
		<_description>Imports Psion 5 series Sheet files</_description>
	</information>
	<loader type="Gnumeric_Builtin:module">
		<attribute name="module_file" value="psiconv.la"/>
	</loader>
	<services>
		<service type="file_opener" id="psiconv" priority="100" probe="TRUE">
			<information>
				<_description>Psion (*.psisheet)</_description>
			</information>
			<file_patterns>
				<file_pattern type="shell_pattern" value="*.psisheet"/>
			</file_patterns>
		</service>
	</services>
</plugin>
