<?xml version="1.0" encoding="UTF-8"?>
<plugin id="Gnumeric_paradox">
	<information>
		<_name>Paradox</_name>
		<_description>Imports Paradox files</_description>
	</information>
	<loader type="Gnumeric_Builtin:module">
		<attribute name="module_file" value="paradox.la"/>
	</loader>
	<services>
		<service type="file_opener" id="paradox" priority="100" probe="FALSE">
			<information>
				<_description>Paradox database or primary index file</_description>
			</information>
			<file_patterns>
				<file_pattern type="shell_pattern" value="*.db"/>
				<file_pattern type="shell_pattern" value="*.px"/>
			</file_patterns>
		</service>
	</services>
</plugin>
