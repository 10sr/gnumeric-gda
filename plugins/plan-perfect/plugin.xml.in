<?xml version="1.0"?>
<plugin id="Gnumeric_plan_perfect">
	<information>
		<_name>Plan Perfect</_name>
		<_description>Imports Plan Perfect Formatted Documents</_description>
	</information>
	<loader type="Gnumeric_Builtin:module">
		<attribute name="module_file" value="plan_perfect.la"/>
	</loader>
	<services>
		<service type="file_opener" id="pln" priority="1" probe="TRUE"
		         open="TRUE" import="TRUE">
			<information>
				<_description>Plan Perfect Format (PLN) import</_description>
			</information>
			<file_patterns>
				<file_pattern type="shell_pattern" value="*.pln"/>
			</file_patterns>
			<save_info saver_id="" format_level="manual"/>
		</service>
	</services>
</plugin>
