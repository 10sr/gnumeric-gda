<?xml version="1.0"?>
<plugin id="Gnumeric_sylk">
	<information>
		<_name>MultiPlan (SYLK)</_name>
		<_description>Imports MultiPlan (SYLK) files</_description>
	</information>
	<loader type="g_module">
		<attribute name="module_file" value="sylk.la"/>
	</loader>
	<services>
		<service type="file_opener" id="sylk" priority="1" probe="TRUE"
			open="FALSE" import="TRUE">
			<information>
				<_description>MultiPlan (SYLK) import</_description>
			</information>
			<save_info saver_id="" format_level="manual_remember"/>
		</service>
	</services>
</plugin>
