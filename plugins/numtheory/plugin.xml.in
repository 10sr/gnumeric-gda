<?xml version="1.0"?>
<plugin id="Gnumeric_numtheory">
	<information>
		<_name>Number Theory</_name>
		<_description>Several basic utilities for prime numbers, pi, phi, sigma. It also hold some simple bitwise operations</_description>
	</information>
	<loader type="g_module">
		<attribute name="module_file" value="numtheory.la"/>
	</loader>
	<services>
		<service type="function_group" id="num_theory">
			<_category>Number Theory</_category>
			<functions>
				<function name="ithprime"/>
				<function name="nt_phi"/>
				<function name="nt_d"/>
				<function name="nt_sigma"/>
				<function name="isprime"/>
				<function name="nt_pi"/>
				<function name="nt_mu"/>
			</functions>
		</service>
		<service type="function_group" id="bitwise">
			<_category>Bitwise Operations</_category>
			<functions>
				<function name="bitor"/>
				<function name="bitxor"/>
				<function name="bitand"/>
				<function name="bitlshift"/>
				<function name="bitrshift"/>
			</functions>
		</service>
	</services>
</plugin>
