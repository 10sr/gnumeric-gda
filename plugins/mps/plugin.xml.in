<?xml version="1.0"?>
<plugin id="Gnumeric_mps">
	<information>
		<_name>Linear and integer program expression format (MPS) module</_name>
		<_description>Reads LP programs stored in the MPS format (*.mps)</_description>
	</information>
	<loader type="Gnumeric_Builtin:module">
		<attribute name="module_file" value="mps.la"/>
	</loader>
	<services>
		<service type="file_opener" id="mps" priority="1" probe="FALSE"
		         open="TRUE" import="TRUE">
			<information>
				<_description>Linear and integer program (*.mps) file format</_description>
			</information>
			<file_patterns>
				<file_pattern type="shell_pattern" value="*.mps"/>
			</file_patterns>
			<save_info saver_id="mps" format_level="manual_remember"/>
		</service>
	</services>
</plugin>
