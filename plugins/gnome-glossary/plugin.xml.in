<?xml version="1.0"?>
<plugin id="Gnumeric_GnomeGlossary">
	<information>
		<_name>Gnome Glossary</_name>
		<_description>It provides support for saving Gnome Glossary in .po files.</_description>
	</information>
	<loader type="python">
		<attribute name="module_name" value="gnome_glossary"/>
	</loader>
	<services>
		<service type="file_saver" id="po" file_extension="po" format_level="auto">
			<information>
				<_description>Gnome Glossary PO file format</_description>
			</information>
		</service>
	</services>
</plugin>
