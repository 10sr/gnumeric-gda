<?xml version="1.0"?>
<plugin id="Gnumeric_sc">
	<information>
		<_name>SC/XSpread</_name>
		<_description>Imports SC/XSpread files</_description>
	</information>
	<loader type="Gnumeric_Builtin:module">
		<attribute name="module_file" value="sc.la"/>
	</loader>
	<services>
		<service type="file_opener" id="sc" priority="1" probe="TRUE"
		         open="FALSE" import="TRUE">
			<information>
				<_description>SC/xspread</_description>
			</information>
			<save_info saver_id="" format_level="manual"/>
		</service>
	</services>
</plugin>
