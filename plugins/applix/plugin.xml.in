<?xml version="1.0"?>
<plugin id="Gnumeric_applix">
	<information>
		<_name>Applix</_name>
		<_description>Imports version 4.[234] spreadsheets</_description>
	</information>
	<loader type="Gnumeric_Builtin:module">
		<attribute name="module_file" value="applix.la"/>
	</loader>
	<services>
		<service type="file_opener" id="applix" priority="100" probe="TRUE"
			open="TRUE" import="TRUE">
			<information>
				<_description>Applix (*.as)</_description>
			</information>
			<file_patterns>
				<file_pattern type="shell_pattern" value="*.as"/>
			</file_patterns>
		</service>
	</services>
</plugin>
