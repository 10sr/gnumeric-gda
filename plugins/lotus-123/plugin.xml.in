<?xml version="1.0" encoding="UTF-8"?>
<plugin id="Gnumeric_lotus">
	<information>
		<_name>Lotus 123</_name>
		<_description>Imports Lotus 123 files</_description>
	</information>
	<loader type="Gnumeric_Builtin:module">
		<attribute name="module_file" value="lotus.la"/>
	</loader>
	<services>
		<service type="file_opener" id="lotus" priority="50" probe="TRUE">
			<information>
				<_description>Lotus 123 (*.wk1)</_description>
			</information>
			<file_patterns>
				<file_pattern type="shell_pattern" value="*.wk1"/>
				<file_pattern type="shell_pattern" value="*.wks"/>
			</file_patterns>
		</service>
	</services>
</plugin>
