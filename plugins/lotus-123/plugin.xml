<?xml version="1.0"?>
<plugin id="Gnumeric_lotus">
	<information name="Lotus 123" description="Imports Lotus 123 files"/>
	<loader type="g_module">
		<attribute name="module_file" value="lotus.so"/>
	</loader>
	<services>
		<service type="file_opener" id="lotus" priority="50" probe="TRUE">
			<information description="Lotus 123 (*.wk1)"/>
			<file_patterns>
				<file_pattern type="shell_pattern" value="*.wk1"/>
				<file_pattern type="shell_pattern" value="*.wks"/>
			</file_patterns>
			<save_info saver_id="" format_level="manual_remember"/>
		</service>
	</services>
</plugin>
