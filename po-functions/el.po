#~ "και 42.7.··Τότε\n"
#~ "\n"
#~ "@SEEALSO=LINEST"

#~ msgid ""
#~ "@FUNCTION=LOGEST\n"
#~ "@SYNTAX=LOGEST(known_y's[,known_x's,const,stat])\n"
#~ "@DESCRIPTION=LOGEST function applies the ``least squares'' method to fit "
