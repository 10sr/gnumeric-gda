  <refentry>
    <refmeta>
      <refentrytitle><anchor id="gnumeric-LOG2">LOG2</refentrytitle><refmiscinfo></refmiscinfo>
    </refmeta>
    <refnamediv>
      <refname>LOG2</refname>
